{
    "name": "dwp",
    "version": "2.4.3",
    "description": "Auto fetch Bing's wallpaper at regular interval",
    "main": "./dist/index.js",
    "engines": {
        "node": ">=12.0.0",
        "npm": ">=7.0.0"
    },
    "bin": {
        "dwp": "./dist/index.js"
    },
    "scripts": {
        "test": "jest",
        "build": "rm -rf ./dist && tsc",
        "prepublishOnly": "npm run build",
        "help": "npm run build && node ./dist/index.js -h",
        "debug": "npm run build && node ./dist/index.js start -i 30m",
        "stop": "npm run build && node ./dist/index.js stop",
        "showlist": "npm run build && node ./dist/index.js list"
    },
    "keywords": [
        "nodejs",
        "wallpaper",
        "crawler",
        "bing",
        "scheduled-job"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/avennn/daily-wallpaper.git"
    },
    "author": {
        "name": "Javen Leung",
        "email": "914301050@qq.com",
        "url": "https://github.com/avennn"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/avennn/daily-wallpaper/issues"
    },
    "homepage": "https://github.com/avennn/daily-wallpaper#readme",
    "dependencies": {
        "@types/node-schedule": "^1.3.1",
        "@types/plist": "^3.0.2",
        "axios": "^0.23.0",
        "chalk": "^4.1.2",
        "cli-table3": "^0.6.0",
        "commander": "^8.2.0",
        "dayjs": "^1.8.23",
        "log4js": "^6.3.0",
        "nc-screen": "^1.4.0",
        "plist": "^3.0.2",
        "ps-node": "^0.1.6",
        "puppeteer": "^2.1.1",
        "table-parser": "^1.0.1",
        "toad-scheduler": "^1.5.0",
        "topparser": "^1.0.3",
        "tori": "^2.0.0"
    },
    "devDependencies": {
        "@tsconfig/node12": "^1.0.7",
        "@types/jest": "^27.0.2",
        "@types/node": "^12.0.0",
        "@types/ps-node": "^0.1.1",
        "@types/puppeteer": "^5.4.3",
        "@types/xml2js": "^0.4.8",
        "@types/yargs": "^16.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "eslint-config-prettier": "^8.2.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^27.3.1",
        "prettier": "^2.2.1",
        "ts-jest": "^27.0.7",
        "typescript": "^4.4.4"
    }
}
